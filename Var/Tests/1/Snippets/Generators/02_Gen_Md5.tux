def description(self):
	# testcase description
	self.setPurpose(purpose=description('summary'))
	self.setRequirement(requirement=description('requirement'))

	# steps description
	self.step1 = self.addStep(expected="md5 generated", description="generate md5",
																			summary="generate md5", enabled=True)
def prepare(self):
	self.LIB_MD5 = SutAdapters.Hashing.MD5(parent=self)
	
def definition(self):
	# starting initial step
	if self.step1.isEnabled():
		self.step1.start()

		hash = self.LIB_MD5.compute(data=input('DATA_MD5'))
		Cache().set(name=input('CACHE_KEY'), data=hash, flag=False)
		
		self.warning("md5: %s" % hash)
		self.step1.setPassed(actual="success")
def cleanup(self, aborted):
	if aborted: self.step1.setFailed(actual=aborted)]]></testdefinition>

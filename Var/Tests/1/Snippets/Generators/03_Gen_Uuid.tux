def description(self):
	# testcase description
	self.setPurpose(purpose=description('summary'))
	self.setRequirement(requirement=description('requirement'))

	# steps description
	self.step1 = self.addStep(expected="uuid generated", description="generate uuid",
																			summary="generate uuid", enabled=True)
def prepare(self):
	self.LIB_UUID = SutAdapters.Identifiers.UUIDS(parent=self)
def definition(self):
	# starting initial step
	if self.step1.isEnabled():
		self.step1.start()

		u = self.LIB_UUID.generate()
		Cache().set(name=input('CACHE_KEY'), data=u, flag=False)
		
		self.warning("uuid: %s" % u)
		self.step1.setPassed(actual="success")
def cleanup(self, aborted):
	if aborted: self.step1.setFailed(actual=aborted)]]></testdefinition>

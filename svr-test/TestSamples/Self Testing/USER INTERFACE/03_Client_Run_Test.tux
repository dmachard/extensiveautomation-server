def description(self):
	# testcase description
	self.setPurpose(purpose="GUI_Automation_Capture")

	# steps description
	self.step1 = self.addStep(expected="Action(s) executed with success", description="Ouverture du répertoire 'Basics'\n- Détection du répertoire 'Do'\n- Ouverture du répertoire 'Basics'\n- Détection du fichier '00_Wait'\n- Ouverture du fichier '00_Wait'\n- Détection de l'ouverture du fichier '00_Wait' dans le workspace\n- Exécution du test\n- Vérification du résultat du test (OK = 1 )", summary="Ouverture du répertoire 'Basics'\n- Détection du répertoire 'Do'\n- Ouverture du répertoire 'Basics'\n- Détection du fichier '00_Wait'\n- Ouverture du fichier '00_Wait'\n- Détection de l'ouverture du fichier '00_Wait' dans le workspace\n- Exécution du test\n- Vérification du résultat du test (OK = 1 )", enabled=True)

def prepare(self):
	# adapters and libraries definitions
	self.ADP_GUI = SutAdapters.Generic.GUI.Sikuli(parent=self, agent=agent('AGENT_GUI'), debug=input('DEBUG'))

	

def definition(self):
	if self.step1.isEnabled():
		self.step1.start()
	
		##CAPTURE>Détection du répertoire 'Basics'
		actionId = self.ADP_GUI.waitClickImage( img=input('IMG_1'), description="Détection du répertoire 'Basics'", similar=input('IMG_1_SIMILAR'), timeout=input('TIMEOUT_GUI') )
		APP_RET1 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET1 is None:
			self.abort("Unable to détection du répertoire 'basics'")
	
		##CAPTURE>Ouverture du répertoire 'Basics'
		actionId = self.ADP_GUI.typeShorcut(key=SutAdapters.Generic.GUI.KEY_RIGHT, modifier=None, special=None, other=None, repeat=0)
		APP_RET2 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET2 is None:
			self.abort("Unable to ouverture du répertoire 'basics'")
	
		##CAPTURE>Détection du répertoire 'Do'
		actionId = self.ADP_GUI.waitClickImage( img=input('IMG_3'), description="Détection du répertoire 'Do'", similar=input('IMG_3_SIMILAR'), timeout=input('TIMEOUT_GUI') )
		APP_RET3 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET3 is None:
			self.abort("Unable to détection du répertoire 'do'")
	
		##CAPTURE>Ouverture du répertoire 'Basics'
		actionId = self.ADP_GUI.typeShorcut(key=SutAdapters.Generic.GUI.KEY_RIGHT, modifier=None, special=None, other=None, repeat=0)
		APP_RET4 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET4 is None:
			self.abort("Unable to ouverture du répertoire 'basics'")
	
		##CAPTURE>Détection du fichier '00_Wait'
		actionId = self.ADP_GUI.waitImage( img=input('IMG_5'), description="Détection du fichier '00_Wait'", similar=input('IMG_5_SIMILAR'), timeout=input('TIMEOUT_GUI') )
		APP_RET5 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET5 is None:
			self.abort("Unable to détection du fichier '00_wait'")
	
		##CAPTURE>Ouverture du fichier '00_Wait'
		actionId = self.ADP_GUI.doubleClickImage( img=input('IMG_6'), description="Ouverture du fichier '00_Wait'", similar=input('IMG_6_SIMILAR') )
		APP_RET6 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET6 is None:
			self.abort("Unable to ouverture du fichier '00_wait'")
	
		##CAPTURE>Détection de l'ouverture du fichier '00_Wait' dans le workspace
		actionId = self.ADP_GUI.waitImage( img=input('IMG_7'), description="Détection de l'ouverture du fichier '00_Wait' dans le workspace", similar=input('IMG_7_SIMILAR'), timeout=input('TIMEOUT_GUI') )
		APP_RET7 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET7 is None:
			self.abort("Unable to détection de l'ouverture du fichier '00_wait' dans le workspace")
	
		##CAPTURE>Exécution du test
		actionId = self.ADP_GUI.clickImage( img=input('IMG_8'), description="Exécution du test", similar=input('IMG_8_SIMILAR') )
		APP_RET8 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET8 is None:
			self.abort("Unable to exécution du test")
	
		##CAPTURE>Vérification du résultat du test (OK = 1 )
		actionId = self.ADP_GUI.waitImage( img=input('IMG_9'), description="Vérification du résultat du test (OK = 1 )", similar=input('IMG_9_SIMILAR'), timeout=input('TIMEOUT_GUI') )
		APP_RET9 = self.ADP_GUI.isActionAccepted(timeout=input('TIMEOUT_GUI'), actionId=actionId)
		if APP_RET9 is None:
			self.abort("Unable to vérification du résultat du test (ok = 1 )")
	
		self.step1.setPassed("test terminated")

def cleanup(self, aborted):
	if aborted: self.step1.setFailed(aborted)
